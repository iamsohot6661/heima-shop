/* 水平间距 */
swiper {
  height: 330;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15px 0px;
}
.nav-list .nav-image {
  width: 128rpx;
  height: 140rpx;
}
.floor-title {
  height: 60rpx;
  width: 100%;
  display: flex;
}
.floor-item .floor-img-box {
  padding-left: 15rpx;
  display: flex;
}
.floor-item .right-img-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}

